---
import SponsorLogo from "../SponsorLogo/SponsorLogo.astro";

import audienceview from "./audienceview.png";

const images = [
  {
    name: "Audienceview",
    alt: "Audienceview",
    lang: "en",
    image: audienceview,
  },
];
---

<div class="flex flex-col gap-6">
  <ul
    role="list"
    class="grid grid-cols-[repeat(auto-fill,minmax(160px,1fr))] gap-9"
  >
    {
      images.map(({ alt, image, lang }) => (
        <SponsorLogo lang={lang} alt={alt} src={image.src} />
      ))
    }
  </ul>
</div>
